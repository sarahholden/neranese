/* --------------------------------------------------------------------------
Hosted Fonts
-----------------------------------------------------------------------------*/

@font-face {
  font-family: "Basis Grotesque Pro";
  src: url(" {{ 'basisGrotesqueMediumPro.eot' | asset_url }} ");
  src: url(" {{ 'basisGrotesqueMediumPro.eot?#iefix' | asset_url }} ") format("embedded-opentype"), url(" {{ 'basisGrotesqueMediumPro.woff2' | asset_url }} ") format("woff2"), url(" {{ 'basisGrotesqueMediumPro.woff' | asset_url }} ") format("woff");
  font-weight: 500;
  font-style: normal;
}

@font-face {
  font-family: "Basis Grotesque Pro";
  src: url(" {{ 'BasisGrotesqueRegular.woff2' | asset_url }} ") format("woff2"), url(" {{ 'BasisGrotesqueRegular.woff' | asset_url }} ") format("woff");
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: "Cardinal Photo";
  src: url(" {{ 'cardinalphotowebItalic.woff2' | asset_url }} ") format("woff2"), url(" {{ 'cardinalphotowebItalic.woff' | asset_url }} ") format("woff");
  font-weight: normal;
  font-style: italic;
}

@font-face {
  font-family: "Cardinal Photo";
  src: url(" {{ 'cardinalphotowebRegular.woff2' | asset_url }} ") format("woff2"), url(" {{ 'cardinalphotowebRegular.woff' | asset_url }} ") format("woff");
  font-weight: normal;
  font-style: normal;
}
